{
  "containerDefinitions": [
    {
      "name": "kirunalabs-backend",
      "image": "191566583164.dkr.ecr.eu-west-3.amazonaws.com/kirunalabs-backend:initial",
      "cpu": 0,
      "portMappings": [
        {
          "name": "kirunalabs-backend-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "entryPoint": ["sh", "-c"],
      "command": ["yarn migrate:deploy && yarn start:prod"],
      "environment": [],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/kirunalabs-backend-preview",
          "awslogs-region": "eu-west-3",
          "awslogs-stream-prefix": "kirunalabs-backend"
        }
      }
    }
  ],
  "family": "kirunalabs-backend",
  "executionRoleArn": "arn:aws:iam::191566583164:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
